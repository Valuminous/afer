{% extends 'base.html.twig' %}

{% block title %}Préfecture{% endblock %}

{% block body %}

<section class="prefecture">

    <p>Nom : {{ prefectures.nomPrefecture }}</p>
    <p>Adresse : {{ prefectures.numeroAdressePrefecture }} {{ prefectures.adressePrefecture }}</p>
    <p>Code postal : {{ prefectures.cp }}</p>
    <p>Commune : {{ prefectures.commune }}</p>
    <p>Préfecture service : {{ prefectures.prefectureService }}</p>

</section>
   <a href="{{ path('prefecture') }}">Retour</a>

{% endblock %}