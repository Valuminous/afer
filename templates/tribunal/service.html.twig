{% extends 'base.html.twig' %}

{% block title %}Liste des services des tribunaux{% endblock %}

{% block body %}

    <h2>Liste des services des tribunaux</h2>
    {% for service in services %}
        <div>{{ service.nom }}</div>
        <div>
            <a href="{{path('tribunal_service_supprimer', {'id':service.id})}}" class="btn btn-danger">Supprimer</a>
            <a href="{{path('tribunal_service_modifier', {'id':service.id})}}" class="btn btn-primary">Modifier</a>
        </div>
    {% endfor %}
    
{% endblock %}
